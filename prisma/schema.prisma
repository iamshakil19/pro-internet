generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid())
  name     String
  email    String
  role     String
  image    String?  @default("")
  phone    String
  password String
  access   Boolean? @default(true)

  @@map("users")
}
